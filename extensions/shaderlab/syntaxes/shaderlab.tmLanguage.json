					"name": "keyword.other"
				}
			},
			"end": "\\b(ENDHLSL)\\b",
			"endCaptures": {
				"1": {
					"name": "keyword.other"
				}
			},
			"patterns": [
				{
					"include": "#hlsl-embedded"
				}
			]
		},
		{
			"name": "string.quoted.double.shaderlab",
			"begin": "\"",
			"end": "\""
		}
